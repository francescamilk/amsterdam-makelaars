<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Url:</strong>
  <%= @post.url %>
</p>

<p>
  <strong>Summary:</strong>
  <%= @post.summary %>
</p>

<p>
  <strong>Body:</strong>
  <%= @post.body %>
</p>

<p>
  <strong>Image:</strong>
  <%= @post.image %>
</p>

<% if account_signed_in? && current_account.admin? %>
  <%= link_to 'Edit', edit_post_path(@post) %> |
  <%= link_to 'Back', admin_posts_path %>
<% end %>